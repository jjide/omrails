<div class="container">
<h1>Listing feeds</h1>

<table>
  <tr class="tb">
    <th>Site Name</th>
    <th>Site Url</th>
    <th>Feed url</th>
   
    
    <th>Latest post title</th>
    
    <!--<th></th>-->
    <th></th>
    <th></th>
  </tr>

<% @feeds.each do |feed| %>
  <tr class="tb">
    <td><%= feed.name %></td>
    <td><%= feed.url %></td>
    <td><%= feed.feed_url %></td>
    
    
    <td><%= feed.latest_post_title %></td>
    
    <!--<td><%= link_to 'Show', feed %></td> -->
    <td><%= link_to 'Edit', edit_feed_path(feed) %></td>
    <td><%= link_to 'Destroy', feed, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Feed', new_feed_path %>
</div>